#include<iostream>
#include<math.h>
using namespace std;
int main()
{   
    int n;
    cin>>n;
    int a=n;
    int div;
    int count=0;
    while(n!=0)
    {
        n=n/10;
        count++;
        
    }
    div = pow(10,count-1);
    while(div!=0)
    {
        int q=a/div;
        cout<<q<<endl;
        
        a=a%div;
        div=div/10;
    }
    return 0;
}
