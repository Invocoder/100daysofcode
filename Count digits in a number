#include<iostream>
#include<math.h>
int main()
{   
    int n , count , div;
    cin>>n;
    count = 0;
    while(n!=0)
    {
        n=n/10;
        count++; //increment count by 1  
    }
    div=pow(10,count-1);
    while(div!=0)
    {
        int q=n/div;
        cout<<q<<endl;
    }
    
    return 0;
}
